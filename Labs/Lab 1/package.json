{
  "name": "torpedodepot-email-highlighter",
  "displayName": "TorpedoDepot Email Highlighter",
  "description": "Highlights TorpedoDepot email addresses using ANTLR parsing",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "torpedodepot-email-highlighter.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "plaintext",
        "aliases": ["Plain Text", "text"],
        "extensions": [".txt"]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "antlr4ts": "antlr4ts -visitor -listener -o src/generated TorpedoDepotEmailFilter.g4"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.4",
    "antlr4ts": "^0.5.0-alpha.4",
    "antlr4ts-cli": "^0.5.0-alpha.4"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4"
  }
}
